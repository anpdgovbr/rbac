{
  "name": "@anpdgovbr/rbac-next",
  "version": "0.2.1",
  "private": false,
  "type": "module",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "description": "Adapter Next.js para RBAC (withApi/withApiForId).",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anpdgovbr/rbac.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc --build",
    "typecheck": "tsc --build --noEmit",
    "pretest": "pnpm run build",
    "test": "node --test dist/test/*.test.js dist/src/test/*.test.js",
    "ncu": "ncu",
    "ncu:check": "ncu",
    "ncu:update": "ncu -u"
  },
  "dependencies": {
    "@anpdgovbr/rbac-core": "^0.1.4",
    "@anpdgovbr/rbac-provider": "^0.1.3"
  },
  "peerDependencies": {
    "react": ">=19"
  },
  "devDependencies": {
    "@types/node": "^20.19.24",
    "@types/react": "^18.3.26",
    "react": "^19.2.0",
    "typescript": "^5.9.3"
  }
}
